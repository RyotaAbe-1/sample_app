<div class="edit">
  <h1>投稿を編集</h1>
  
  <%= form_with model:@list, url:"/todolists/#{@list.id}", local:true do |f| %>
    
    <h3>タイトル</h3>
    <%= f.text_field :title %>
     
    <h3>本文</h3>
    <%= f.text_area :body %>
    <h3>画像更新</h3>
    <%= f.attachment_field :image %>
    <%= f.submit '保存' %>
  <% end %>
  
</div>  